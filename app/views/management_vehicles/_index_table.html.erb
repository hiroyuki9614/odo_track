<div>
  <table border="1" >
    <tr>
      <th><%= t('activerecord.attributes.vehicle.created_at') %></th>
      <th><%= t('activerecord.attributes.vehicle.vehicle_name') %></th>
      <th><%= t('activerecord.attributes.vehicle.number') %></th>
      <th><%= t('activerecord.attributes.vehicle.manufacture') %></th>
      <th><%= t('activerecord.attributes.vehicle.current_drive_distance') %></th>
    </tr>
    <tr>
      <td><%= vehicle.created_at.strftime("%Y年%m月%d日") %><%= vehicle.week_format %></td>
      <td><%= vehicle.vehicle_name %></td>
      <td><%= vehicle.number %></td>
      <td><%= vehicle.manufacture %></td>
      <td><%= "#{number_with_delimiter(vehicle.current_drive_distance)} km" %></td>
    </tr>
  </tr>
</table>
</div>
